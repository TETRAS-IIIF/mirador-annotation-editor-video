{
  "name": "mirador-annotation-editor",
  "version": "1.1.3-rc14",
  "description": "Mirador annotation editor plugin in a React component. Mirador 4 compatible",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "sideEffects": ["dist/index.css"],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/index.css": "./dist/index.css"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "vite build && npm run types:copy",
    "types:copy": "mkdir -p dist && cp src/index.d.ts dist/index.d.ts",
    "clean": "rm -rf ./dist && rm -rf node_modules",
    "lint": "npx eslint ./",
    "prepublishOnly": "npm run build",
    "start": "vite",
    "test": "npx vitest run",
    "test:coverage": "npm run lint && npx vitest run --coverage",
    "build:demo": "GITHUB_PAGES=true vite build -c vite.demo.config.js"
  },
  "contributors": [
    "Anthony Geourjon <anthony.geourjon@tetras-libre.fr> (https://www.tetras-libre.fr)",
    "David Rouquet <david.rouquet@tetras-libre.fr> (https://www.tetras-libre.fr)",
    "Antoine Roy <antoine.roy@tetras-libre.fr> (https://www.tetras-libre.fr)"
  ],
  "dependencies": {
    "axios": "^1.6.7",
    "babel-loader": "^9.1.3",
    "brace": "^0.11.1",
    "draft-js": "^0.11.6",
    "draft-js-export-html": "^1.4.1",
    "draft-js-import-html": "^1.4.1",
    "html-loader": "^5.0.0",
    "i18next": "^25.4.1",
    "json-edit-react": "^1.22.2",
    "react-color": "^2.19.3",
    "react-i18next": "^15.7.4",
    "react-konva": "^19.0.10",
    "react-konva-to-svg": "^1.0.2",
    "react-quill": "npm:react-quill-new@3.6.0",
    "react-redux": "^9.2.0",
    "react-refresh": "^0.14.0",
    "react-resize-observer": "^1.1.1",
    "react-select": "^5.10.1",
    "react-sortablejs": "^6.1.4",
    "redux": "^5.0.1",
    "redux-saga": "^1.3.0",
    "redux-thunk": "^3.1.0",
    "sortablejs": "^1.15.2",
    "use-image": "^1.1.1"
  },
  "peerDependencies": {
    "react": ">=19",
    "react-dom": ">=19",
    "mirador": "npm:mirador-video@1.2.1",
    "@mui/material": ">=5",
    "@mui/system": ">=5",
    "@emotion/react": ">=11",
    "@emotion/styled": ">=11",
    "lodash": "^4.17.11",
    "prop-types": "^15.7.2",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.9",
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.10.4",
    "@mui/icons-material": "^7.0.0",
    "@mui/material": "^7.x",
    "@mui/utils": "^5.13.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/coverage-v8": "^3.0.5",
    "canvas": "^2.6.1",
    "css-loader": "^6.10.0",
    "enzyme": "^3.11.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.20.3",
    "happy-dom": "^20.0.0",
    "jest": "^30.2.0",
    "jest-canvas-mock": "^2.2.0",
    "jest-junit": "^15.0.0",
    "jest-localstorage-mock": "^2.4.2",
    "mirador": "^4.0.0",
    "prop-types": "^15.7.2",
    "react": "^19.0.0",
    "react-dom": "^19.0",
    "style-loader": "^3.3.4",
    "uuid": "^8.2.0"
  },
  "license": "Apache-2.0"
}
